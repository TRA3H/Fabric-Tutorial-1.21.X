{
	"schemaVersion": 1,
	"id": "tutorialmod",
	"version": "${version}",
	"name": "Tutorial Mod",
	"description": "Tutorial Mod by Cyrus",
	"authors": [
		"Cyrus"
	],
	"contact": {
		"homepage": "https://cyrus.net/",
		"sources": "https://github.com/TRA3H/Fabric-Tutorial-1.21.X"
	},
	"license": "MIT",
	"icon": "assets/tutorialmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.cyrus.tutorialmod.TutorialMod",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.cyrus.tutorialmod.TutorialModDataGenerator",
				"adapter": "kotlin"
			}
		],
		"client": [
			"net.cyrus.tutorialmod.TutorialModClient"
		]
	},
	"mixins": [
		"tutorial-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}